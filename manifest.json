{
    "manifest_version": 2,
    "name": "Inst Follow/Unfollow",  
    "description": "follow and unfollow users by Tag",  
	"version": "1.0", 
	
	"permissions": ["tabs","http://*/*","storage","alarms","activeTab", "<all_urls>"],


	

    "browser_action": { 
		"default_icon": {
			"19": "images/icons/19.png",
			"38": "images/icons/38.png"
		},
        "default_popup": "interface.html"  
    },
	

	  "background": {
		"scripts": ["background.js"]
	  },
	"icons": {
		"16": "images/icons/16.png",
		"19": "images/icons/19.png",
		"38": "images/icons/38.png",
		"64": "images/icons/64.png",
		"128": "images/icons/128.png"
	}
}